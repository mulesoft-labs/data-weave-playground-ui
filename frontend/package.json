{
  "name": "@mulesoft/data-weave-playground",
  "version": "0.0.0",
  "description": "Dataweave Workbench",
  "main": "src/index.tsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mulesoft/data-weave-playground.git"
  },
  "scripts": {
    "start": "npm run clear:build && node webpack/dev-server.js",
    "clean": "rm -rf build && rm -rf dist ",
    "clear:build": "npm run clean && mkdir build && mkdir dist",
    "test": "jest --verbose",
    "build:aliases": "node scripts/aliasesGenerator.js webpack/aliases.js -j package.json -t tsconfig.json",
    "build:static": "npm run clear:build && webpack --config webpack/config.production.js",
    "deploy": "AWS_ACCESS_KEY_ID=$AWS_ACCESS_KEY_ID AWS_SECRET_ACCESS_KEY=$AWS_SECRET_ACCESS_KEY s3-deploy './dist/*.zip' --cwd './dist/' --bucket dataweave-playground"
  },
  "jest": {
    "globals": {
      "JEST_ENV": true
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*\\.spec)\\.(ts|tsx|js)$",
    "mapCoverage": true,
    "collectCoverageFrom": [
      "**src/**/__tests__/*.{ts,tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^worker-loader.*": "<rootDir>/src/__test__/workerMock.ts",
      "^components": "<rootDir>/src/components",
      "^common/types": "<rootDir>/src/app/common/types",
      "^gist/types": "<rootDir>/src/app/gist/types",
      "^gist/actions": "<rootDir>/src/app/gist/actions",
      "^gist/reducer": "<rootDir>/src/app/gist/reducer",
      "^common/treeNode/types": "<rootDir>/src/app/common/treeNode/types",
      "^dataweave/types": "<rootDir>/src/app/dataweave/types",
      "^project/types": "<rootDir>/src/app/project/types",
      "^common/reducer": "<rootDir>/src/app/common/reducer",
      "^dataweave/reducer": "<rootDir>/src/app/dataweave/reducer",
      "^project/reducer": "<rootDir>/src/app/project/reducer",
      "^dataweave/actions": "<rootDir>/src/app/dataweave/actions",
      "^project/actions": "<rootDir>/src/app/project/actions",
      "^common/helpers": "<rootDir>/src/app/common/helpers",
      "^dataweave/helpers": "<rootDir>/src/app/dataweave/helpers",
      "^project/helpers": "<rootDir>/src/app/project/helpers",
      "^dataweave/selectors": "<rootDir>/src/app/dataweave/selectors",
      "^project/selectors": "<rootDir>/src/app/project/selectors",
      "^use_cases*": "<rootDir>/src/use_cases/dataweave*",
      "^managers(.*)": "<rootDir>/src/managers$1",
      "^components(.*)": "<rootDir>/src/components$1",
      "^fileSystem/types": "<rootDir>/src/app/fileSystem/types",
      "^fileSystem/reducer": "<rootDir>/src/app/fileSystem/reducer",
      "^fileSystem/actions": "<rootDir>/src/app/fileSystem/actions",
      "^fileSystem/selectors": "<rootDir>/src/app/fileSystem/selectors",
      "^use_cases(.*)": "<rootDir>/src/use_cases$1",
      "^ui/types": "<rootDir>/src/app/ui/types",
      "^ui/reducer": "<rootDir>/src/app/ui/reducer",
      "^ui/actions": "<rootDir>/src/app/ui/actions",
      "^ui/selectors": "<rootDir>/src/app/ui/selectors",
      "^gist/selectors": "<rootDir>/src/app/gist/selectors",
      "^fileSystem/helpers": "<rootDir>/src/app/fileSystem/helpers",
      "^gist/helpers": "<rootDir>/src/app/gist/helpers",
      "monaco-editor": "<rootDir>/node_modules/monaco-editor/min/vs/loader.js"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@mulesoft/anypoint-components": "2.80.0",
    "@mulesoft/anypoint-icons": "4.4.2",
    "@mulesoft/anypoint-navbar": "0.8.0",
    "@mulesoft/anypoint-styles": "2.5.3",
    "@mulesoft/data-weave-monaco": "latest-3.1.1",
    "@mulesoft/dw-parser-js": "latest-2.4.0",
    "@types/jest": "21.1.1",
    "@types/moment": "^2.13.0",
    "@types/node": "8.0.24",
    "@types/react": "15.6.15",
    "@types/react-dom": "^15.5.1",
    "@types/react-redux": "5.0.14",
    "body-parser": "1.17.2",
    "classnames": "2.2.5",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.1",
    "express": "4.15.3",
    "extract-text-webpack-plugin": "3.0.0",
    "flareon": "1.0.2-1",
    "fork-ts-checker-webpack-plugin": "0.2.7",
    "happypack": "4.0.0-beta.2",
    "http-proxy-middleware": "0.17.4",
    "jest": "20.0.4",
    "js-base64": "2.3.2",
    "moment": "^2.19.3",
    "monaco-editor": "0.15.6",
    "oddish": "1.0.2-1",
    "p-debounce": "1.0.0",
    "postcss-cssnext": "3.0.2",
    "postcss-import": "10.0.0",
    "postcss-loader": "2.0.6",
    "postcss-nested": "2.1.2",
    "postcss-simple-extend": "1.0.0",
    "postcss-simple-vars": "4.1.0",
    "prettier": "1.5.3",
    "prop-types": "15.5.10",
    "query-string": "^5.0.1",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-dropzone": "4.2.1",
    "react-hot-loader": "1.3.1",
    "react-panelgroup": "1.0.2",
    "react-redux": "5.0.5",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redux-batched-actions": "^0.1.6",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "s3-deploy": "0.7.5",
    "source-map-loader": "0.2.1",
    "style-loader": "0.18.2",
    "superagent": "3.5.2",
    "ts-jest": "21.1.4",
    "ts-loader": "3.1.1",
    "typescript": "^3.5.1",
    "webpack": "3.8.1",
    "webpack-dev-middleware": "1.12.0",
    "webpack-dev-server": "2.9.4",
    "webpack-hot-middleware": "2.20.0",
    "worker-loader": "1.1.0",
    "zip-webpack-plugin": "2.0.0"
  }
}
